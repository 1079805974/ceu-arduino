#include "out.ceu"
#include "wclock.ceu"

output high/low OUT_03;
var int value = _;

_Serial.begin(9600);

_pinMode(_A2, _INPUT);

loop do 

    value = _analogRead(_A2);
    _Serial.println(value);
    _Serial.flush();
    if value < 30 then   // if it's dark
        emit OUT_03(high); //light the LED
    else
        emit OUT_03(low);
    end

    await 500ms;
end

