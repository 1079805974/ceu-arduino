#include "i2c.ceu"
#include "wclock.ceu"

_Serial.begin(9600);

emit I2C(on, 8);

par do
    loop do
        var[20] byte str = [];
        await I2C_Read(&str, _, _);
        var u8 i;
        loop i in [1-> $str as u8] do
            _Serial.println(rx_buf[i-1]);
            _Serial.flush();
        end
    end
with
    loop do
        var[20] byte str = [].."43210";
        await I2C_Write(&str, _);
    end
end