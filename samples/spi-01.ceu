#include "wclock/timer1.ceu"
#include "spi.ceu"

output on/off PIN_09;

_Serial.begin(9600);
_Serial.println("=");

watching Spi() do
    loop do
        emit PIN_09(off);
        await (50)ms;
        emit PIN_09(on);
        await (50)ms;
        watching SPI_Transaction(1400000, SPI_MSBFIRST, SPI_MODE2) do
            var u8 received_data = await SPI_Transfer(_);
            _Serial.println("Data is:");_Serial.println(received_data,{BIN});
        end
    end
end
