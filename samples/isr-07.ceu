#include "timer.ceu"

input int PIN02;
#include "pins.ceu"

_pinMode(13, _OUTPUT);
_pinMode( 2, _INPUT);

loop do
    watching PIN02 do
        var int x = 0;
        every 500ms do
            x = not x;
            _digitalWrite(13, x);
        end
    end
    await 1s;     // debouncing
    await PIN02;
    await 1s;     // debouncing
end
